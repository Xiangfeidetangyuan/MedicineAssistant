<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.unboxing.medicineassistant">

    <uses-permission android:name="android.permission.READ_CALENDAR" />
    <uses-permission android:name="android.permission.WRITE_CALENDAR" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".changePwd"/>
        <activity android:name=".fogetPwd" />
        <activity android:name=".register" />
        <activity android:name=".editinform"/>
        <activity android:name=".MedicineInform"/>
        <activity android:name=".searchMedicine" />
        <activity android:name=".InformList" />
        <activity android:name=".InformControl" />
        <activity android:name=".SetInform" />
        <activity android:name=".activity.ListAllActivity" />
        <activity android:name=".activity.ShowItemActivity" />
    </application>

</manifest>